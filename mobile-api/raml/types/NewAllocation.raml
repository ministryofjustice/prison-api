#%RAML 1.0 DataType
description: New Allocation
properties:
  bookingId:
    type: number
    (javaType): Long
    description: Offender Booking Id
  staffId:
    type: number
    (javaType): Long
    description: Keyworker's staff Id
  type:
    type: string
    description: Whether auto or manual
    pattern: "[AM]"
    maxLength: 1
  reason?:
    type: string
    description: Allocation reason
    pattern: \w*
    maxLength: 12
