{
  "role": {
    "name": "prison-api"
  },
  "customDimensions": {
    "service.version": "${BUILD_NUMBER}"
  },
  "instrumentation": {
    "logging": {
      "level": "DEBUG"
    }
  },
  "selfDiagnostics": {
    "destination": "console"
  },
  "sampling": {
    "percentage": 100
  },
  "preview": {
    "sampling": {
      "overrides": [
        {
          "attributes": [
            {
              "key": "db.system",
              "value": "oracle",
              "matchType": "strict"
            }
          ],
          "percentage": 100
        },
        {
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 100
        }
      ]
    }
  }
}
